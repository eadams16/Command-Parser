CREATE ELEVATOR elevator1 WITH LIMIT 30 SPEED 10 ACCELERATION 0.1
CREATE ELEVATOR elevator3 WITH LIMIT 30 SPEED 10 ACCELERATION 0.15
DECLARE ELEVATOR CONTROLLER elevator_controller2 WITH ELEVATORS elevator1 elevator3
DECLARE BUS elevator_bus1 WITH CONTROLLER elevator_controller2
COMMIT
@CLOCK 0.5
DO elevator_controller2 DEFLECT ELEVATOR 30 UP
@WAIT 40
DO elevator_controller2 DEFLECT ELEVATOR 30 DOWN
@WAIT 60
@EXIT