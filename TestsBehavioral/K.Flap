CREATE SPLIT FLAP flap_split1 WITH LIMIT 50 SPEED 5 ACCELERATION 0.15
CREATE SPLIT FLAP flap_split3 WITH LIMIT 50 SPEED 4 ACCELERATION 0.15
DECLARE FLAP CONTROLLER flap_controller2 WITH FLAPS flap_split1 flap_split3
DECLARE BUS flap_bus1 WITH CONTROLLER flap_controller2
COMMIT
@CLOCK 0.5
DO flap_controller2 DEFLECT FLAP 3
@WAIT 32
DO flap_controller2 DEFLECT FLAP UP
@WAIT 31
@EXIT