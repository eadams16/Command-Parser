CREATE RUDDER rudder1 WITH LIMIT 45 SPEED 10 ACCELERATION 0.1
DECLARE RUDDER CONTROLLER rudder_controller1 WITH RUDDER rudder1
DECLARE BUS rudder_bus1 WITH CONTROLLER rudder_controller1
COMMIT
@CLOCK 1
DO rudder_controller1 DEFLECT RUDDER 45 LEFT
@WAIT 43
DO rudder_controller1 DEFLECT RUDDER 45 RIGHT
@WAIT 65
DO rudder_controller1 DEFLECT RUDDER 0 LEFT
@WAIT 45
DO rudder_controller1 DEFLECT RUDDER 30 LEFT
@WAIT 17
DO rudder_controller1 DEFLECT RUDDER 45 RIGHT
@WAIT 80
@EXIT